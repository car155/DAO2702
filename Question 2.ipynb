   "source": [
    "def is_palindrome(integer):\n",
    "    forwards = str(integer)\n",
    "    backwards = forwards[::-1]\n",
    "    return forwards == backwards\n",
    "\n",
    "eb[1].check(is_palindrome)"
   ]
