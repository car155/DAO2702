   "source": [
    "def longest_word(words):\n",
    "    words = words.split()\n",
    "    longest = \"\"\n",
    "    for word in words:\n",
    "        if len(word) > len(longest):\n",
    "            longest = word\n",
    "    return longest\n",
    "\n",
    "eb[16].check(longest_word)"
