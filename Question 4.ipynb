   "source": [
    "def is_prime(n):\n",
    "    prev_numbers = range(2, n - 1)\n",
    "    prime = True\n",
    "    for num in prev_numbers:\n",
    "        if n % num == 0:\n",
    "            prime = False\n",
    "            break\n",
    "    return prime\n",
    "\n",
    "eb[3].check(is_prime)"
   ]
