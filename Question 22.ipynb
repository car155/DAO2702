   "source": [
    "def value_count(ls):\n",
    "    dict = {}\n",
    "    for value in ls:\n",
    "        if value in dict:\n",
    "            dict[value]+=1\n",
    "        else: \n",
    "            dict[value] = 1\n",
    "    return dict\n",
    "\n",
    "eb[21].check(value_count)"
