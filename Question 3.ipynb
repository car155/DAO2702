   "source": [
    "def seven_not_five(n):\n",
    "    sevens = range(7, n + 1, 7)\n",
    "    fives = range(5, n + 1, 5)\n",
    "    return [num for num in sevens if num not in fives]\n",
    "\n",
    "eb[2].check(seven_not_five)"
   ]
